  <file original='locale/%lang.properties' datatype='javapropertyresourcebundle'
		xml:space='preserve' source-language='en' target-language='fr'
		product-name='Linkificator' product-version='0.8.0'>
	<body>
	  <group id='stats'>
		<trans-unit id='stats.links[zero]'>
		  <source>0 links processed</source>
		  <target state='translated'>0 liens traités</target>
		</trans-unit>
		<trans-unit id='stats.links[one]'>
		  <source>1 link processed</source>
		  <target state='translated'>1 lien traité</target>
		</trans-unit>
		<trans-unit id='stats.links'>
		  <source>%d links processed</source>
		  <target state='translated'>%d liens traités</target>
		</trans-unit>
		<trans-unit id='stats.time'>
		  <source>in %d ms</source>
		  <target state='translated'>en %d ms</target>
		</trans-unit>
		<trans-unit id='stats.excluded'>
		  <source>page excluded</source>
		  <target state='translated'>page ignorée</target>
		</trans-unit>
		<trans-unit id='stats.not_processed'>
		  <source>page not processed</source>
		  <target state='translated'>page non traitée</target>
		</trans-unit>
	  </group>
	</body>
  </file>
